{
    "manifest_version": 3,
    "name": "TikTok Peter Focus",
    "version": "1.0",
    "description": "Add the TikTok Peter Focus extension to Chrome Extension documentation articles",
    "icons": {
        "16": "images/logo@16px.png",
        "32": "images/logo@32px.png",
        "48": "images/logo@48px.png",
        "128": "images/logo@128px.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "*"
          ]
        }
      ]
}